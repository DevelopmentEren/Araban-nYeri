@model IEnumerable<Student>

<!doctype html>
<html>
<head>

	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>List</title>
	<link rel="icon" href="~/car.png" />
</head>

<body>
	<table border="1">
		<thead>
		<th>Name</th>
		<th>Email</th>
		<th>Phone</th>
		</thead>
		@foreach (var item in Model)
		{
		<tr>
			<td>@item.Name</td>
			<td>@item.Email</td>
			<td>@item.Phone</td>
		</tr>
		}
	</table>
</body>
</html>